<h1>Listing trial_models</h1>

<table>
  <thead>
    <tr>
      <th>Trial attribute</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trial_models.each do |trial_model| %>
      <tr>
        <td><%= trial_model.trial_attribute %></td>
        <td><%= link_to 'Show', trial_model %></td>
        <td><%= link_to 'Edit', edit_trial_model_path(trial_model) %></td>
        <td><%= link_to 'Destroy', trial_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trial model', new_trial_model_path %>
